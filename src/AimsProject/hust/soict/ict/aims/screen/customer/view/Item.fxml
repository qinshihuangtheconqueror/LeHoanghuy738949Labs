<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <VBox alignment="CENTER" prefHeight="175.0" prefWidth="305.0">
         <children>
            <Label fx:id="lblTitle" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" text="Title" />
            <Label fx:id="lblCost" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" text="Cost" />
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="305.0" spacing="20.0">
         <children>
            <Button fx:id="btnAddtoCart" mnemonicParsing="false" onAction="#btnAddtoCartClicked" prefHeight="50.0" prefWidth="305.0" text="Add to Cart" />
            <Button fx:id="btnPlay" alignment="CENTER" mnemonicParsing="false" onAction="#btnPlayClicked" prefHeight="50.0" prefWidth="305.0" text="Play" />
         </children>
      </HBox>
   </children>
</AnchorPane>
